<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">
    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@layout/activity_register"
        motion:constraintSetStart="@layout/activity_register_sp"
        motion:duration="1500"

        >
        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="65"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/imgView_hart"
                motion:percentX="0"
                motion:percentY="0" />

            <KeyPosition
                motion:framePosition="65"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/imgView_hands"
                motion:percentX="0"
                motion:percentY="0" />

            <KeyPosition
                motion:framePosition="75"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/imgView_hart"
                motion:percentX="1"
                motion:percentY="1" />

            <KeyPosition
                motion:framePosition="75"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/imgView_hands"
                motion:percentX="1"
                motion:percentY="1" />

            <KeyAttribute
                android:rotation="0"
                motion:framePosition="0"
                motion:motionTarget="@+id/imgView_hands"

                />

            <KeyAttribute
                android:rotation="180"
                motion:framePosition="85"
                motion:motionTarget="@+id/imgView_hands"

                />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="75"
                motion:motionTarget="@+id/linear_inputs_register" />

            <KeyAttribute
                android:alpha="1"
                motion:framePosition="85"
                motion:motionTarget="@+id/linear_inputs_register" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="85"
                motion:motionTarget="@+id/linear_icons" />

            <KeyAttribute
                android:alpha="1"
                motion:framePosition="95"
                motion:motionTarget="@+id/linear_icons" />

            <KeyPosition

                motion:framePosition="85"
                motion:motionTarget="@+id/tv_backLogin"
                motion:percentX="0"
                motion:percentY="0"

                />

            <KeyPosition

                motion:framePosition="95"
                motion:motionTarget="@+id/tv_backLogin"
                motion:percentX="1"
                motion:percentY="1"

                />
        </KeyFrameSet>
    </Transition>
<!--    <ConstraintSet android:id="@layout/activity_register_sp"></ConstraintSet>-->

<!--    <ConstraintSet android:id="@layout/activity_register"></ConstraintSet>-->
</MotionScene>